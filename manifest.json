{
"manifest_version": 2,
    "name": "Blood Money",
    "description": "The puppets of the NRA. This extension replaces images of mass shooters with one of the 20 top career recipients of NRA funding.",
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" },
    "version": "1.3",
    "web_accessible_resources": [
        "/*.png"
      ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
