{
"manifest_version": 2,
    "name": "Redact List",
    "description": "Let's stop giving mass shooters publicity.",
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" },
    "version": "1.2",
    "web_accessible_resources": [
        "/*.png"
      ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
